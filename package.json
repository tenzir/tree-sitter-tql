{
  "name": "tree-sitter-tql",
  "version": "0.1.0",
  "description": "The Tenzir Query Language (TQL) is a pipeline-style dataflow language.",
  "repository": "https://github.com/tenzir/tree-sitter-tql",
  "license": "MIT",
  "author": {
    "name": "Tenzir",
    "email": "engineering@tenzir.com",
    "url": "https://tenzir.com/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "tql"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "markdownlint-cli": "^0.45.0",
    "prebuildify": "^6.0.1",
    "prettier": "^3.6.2",
    "tree-sitter-cli": "^0.25.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
